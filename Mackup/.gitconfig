[core]
	editor = vim
	quotepath = false
[user]
	name = zcjava
	email = zcjava@qq.com
[merge]
	tool = vimdiff
[alias]
	unstage = reset HEAD --
    cm = commit -m 
	last = log -1 HEAD
	st = status
	co = checkout
	br = branch
	ci = commit
    pl = pull --rebase
    ps = push 
	l = log --abbrev-commit --graph
[pull]
	rebase = false
[http]
	postBuffer = 524288000
	lowSpeedLimit = 0
	lowSpeedTime = 999999
	sslVerify = false
	proxy = http://127.0.0.1:7890
[https]
	proxy = http://127.0.0.1:7890
