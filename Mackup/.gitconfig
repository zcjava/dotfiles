[merge]
	tool = vimdiff
[alias]
	unstage = reset HEAD --
    cm = commit -m 
	last = log -1 HEAD
	st = status
	co = checkout
	br = branch
	ci = commit
    pl = pull --rebase
    ps = push 
	l = log --abbrev-commit --graph
[pull]
	rebase = false
